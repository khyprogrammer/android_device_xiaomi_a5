# Android fstab file for TWRP
/dev/block/by-name/boot        /boot     emmc   defaults  defaults
/dev/block/mapper/system       /system   erofs  ro        wait,slotselect
/dev/block/by-name/userdata    /data     f2fs   nosuid,nodev,noatime,errors=panic   wait,check,quota,formattable,resize
/dev/block/by-name/rescue      /cache    ext4   nosuid,nodev,noatime   wait,check,formattable


